#include <stdio.h>
#include <stdlib.h>
struct Node {int data; struct Node* next;};
struct Node* head = NULL; 

void insert(int x){struct Node* n=malloc(sizeof(struct Node));n->data=x;n->next=head;head=n;}
void del(){if(head){struct Node* t=head;head=head->next;free(t);}}
void display(){struct Node* t=head;while(t){printf("%d ",t->data);t=t->next;}printf("\n");}

int main(){
    insert(10); insert(20); insert(30);
    printf("List: "); display();
    del(); printf("After deletion: "); display();
    return 0;
}
