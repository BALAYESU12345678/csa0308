#include<stdio.h>
int part(int a[],int l,int h){int p=a[h],i=l-1,t;
    for(int j=l;j<h;j++)if(a[j]<p){t=a[++i];a[i]=a[j];a[j]=t;}
    t=a[i+1];a[i+1]=a[h];a[h]=t;return i+1;}
void qs(int a[],int l,int h){if(l<h){int pi=part(a,l,h);qs(a,l,pi-1);qs(a,pi+1,h);}}
int main(){int a[5]={5,3,4,1,2};qs(a,0,4);for(int i=0;i<5;i++)printf("%d ",a[i]);}
