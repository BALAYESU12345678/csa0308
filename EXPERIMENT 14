#include <stdio.h>
#include <stdlib.h>
struct node{int d;struct node *l,*r;};
struct node* new(int v){struct node* n=malloc(sizeof(struct node));n->d=v;n->l=n->r=NULL;return n;}
void inorder(struct node* r){if(!r)return;inorder(r->l);printf("%d ",r->d);inorder(r->r);}
void preorder(struct node* r){if(!r)return;printf("%d ",r->d);preorder(r->l);preorder(r->r);}
void postorder(struct node* r){if(!r)return;postorder(r->l);postorder(r->r);printf("%d ",r->d);}
int main(){struct node* r=new(1);r->l=new(2);r->r=new(3);r->l->l=new(4);r->l->r=new(5);
    printf("Inorder: ");inorder(r);
    printf("\nPreorder: ");preorder(r);
    printf("\nPostorder: ");postorder(r);}
