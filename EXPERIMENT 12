#include <stdio.h>
#include <ctype.h>
char stack[50]; int top=-1;
void push(char x){ stack[++top]=x; }
char pop(){ return stack[top--]; }
int priority(char x){ return (x=='+'||x=='-')?1:(x=='*'||x=='/')?2:0; }

int main(){
    char exp[50], *e, x; printf("Enter infix: "); scanf("%s",exp);
    e=exp; while(*e){
        if(isalnum(*e)) printf("%c",*e);
        else if(*e=='(') push(*e);
        else if(*e==')'){ while((x=pop())!='(') printf("%c",x); }
        else { while(top!=-1 && priority(stack[top])>=priority(*e)) printf("%c",pop()); push(*e); }
        e++;
    }
    while(top!=-1) printf("%c",pop());
    return 0;
}
