#include<stdio.h>
int main(){
    int g[5][5]={{0,10,0,30,100},{10,0,50,0,0},{0,50,0,20,10},{30,0,20,0,60},{100,0,10,60,0}};
    int v[5]={0},d[5],i,j,u,min;
    for(i=0;i<5;i++)d[i]=999;d[0]=0;
    for(i=0;i<5;i++){min=999;
      for(j=0;j<5;j++)if(!v[j]&&d[j]<min)min=d[j],u=j;
      v[u]=1;for(j=0;j<5;j++)if(g[u][j]&&!v[j]&&d[u]+g[u][j]<d[j])d[j]=d[u]+g[u][j];}
    for(i=0;i<5;i++)printf("0->%d=%d\n",i,d[i]);
}
