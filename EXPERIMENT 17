#include<stdio.h>
void merge(int a[],int l,int m,int r){int i=l,j=m+1,k=0,b[10];
    while(i<=m&&j<=r)b[k++]=a[i]<a[j]?a[i++]:a[j++];
    while(i<=m)b[k++]=a[i++];while(j<=r)b[k++]=a[j++];
    for(i=0;i<k;i++)a[l+i]=b[i];}
void ms(int a[],int l,int r){if(l<r){int m=(l+r)/2;ms(a,l,m);ms(a,m+1,r);merge(a,l,m,r);}}
int main(){int a[5]={5,3,4,1,2};ms(a,0,4);for(int i=0;i<5;i++)printf("%d ",a[i]);}
